  "engines": {
    "node": ">= 0.12.0"
  },
  "preferGlobal": true,
  "dependencies": {
    "bower": "^1.3.12",
    "chalk": "^0.5.1",
    "commander": "^2.6.0",
    "fs.extra": "^1.3.2",
    "inquirer": "^0.8.0",
    "npm-package-arg": "^3.0.0",
    "request": "^2.51.0",
    "rimraf": "^2.2.8",
    "semver": "^4.2.0"
  },
  "devDependencies": {
    "chai": "^2.0.0",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.5",
    "lcov-filter": "0.0.1",
    "mocha": "^2.1.0",
    "sinon": "^1.12.2"
  },
  "scripts": {
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec",
